toggle = false

Effect.Enabled = false
function onKeyPress(inputObject, gameProcessedEvent)
  if inputObject.KeyCode == Enum.KeyCode.RightControl then	
	  if toggle == false then
		  setting.IncomingReplicationLag = 1000
		  Effect.Enabled = true
		  toggle = true
	  else
